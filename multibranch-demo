pipeline{
agent any
stages {
stage ('Maven Build'){
Steps {
sh "mvn clean package"
}
}
stage ('build and test dev delopy'){
when {
       branch 'develop'
      }
Steps {
echo "delopy into development branch"
}
}
stage ('build and test release branch'){
when {
      branch 'release'
      }
Steps {
echo " deploy into release branch"
}
}
stage ('build and test master branch'){
when {
       branch 'master'
     }
Steps {
echo "build deploy into production branch"
}
}
}
}
