pipeline{
agent any
stages {
stage ('Maven Build'){
Steps {
sh "mvn clean package"
}
}
stage ('build and test delopy to dev'){
when {
       branch 'develop'
      }
Steps {
echo "delopy into development enviroment"
}
}
stage ('depoy to release'){
when {
      branch 'release'
      }
Steps {
echo " deploy into release enviroment"
}
}
stage ('deploy to master'){
when {
       branch 'master'
     }
Steps {
echo "deploy into production enviroment"
}
}
}
}
